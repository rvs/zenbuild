kernel:
  image: KERNEL_TAG
  cmdline: "rootdelay=3"
init:
  - GRUB_TAG
  - DTREES_TAG
  - XEN_TAG
  - GPTTOOLS_TAG
  - DOM0ZTOOLS_TAG
  - linuxkit/init:v0.3
  - linuxkit/runc:v0.3
  - linuxkit/containerd:v0.3
  - linuxkit/getty:v0.3
  - KMOD_PFRING_TAG
onboot:
   - name: sysctl
     image: linuxkit/sysctl:v0.3
   - name: modprobe
     image: linuxkit/modprobe:v0.3
     command: ["modprobe", "-a", "nct6775", "w83627hf_wdt"]
services:
   - name: dhcpcd
     image: linuxkit/dhcpcd:v0.3
     binds:
       - /run:/etc
     command: ["/sbin/dhcpcd", "--nobackground", "-f", "/dhcpcd.conf", "-z", "eth*,wlan*"]
   - name: ntpd
     image: linuxkit/openntpd:v0.3
   - name: wwan
     image: WWAN_TAG
   - name: wlan
     image: WLAN_TAG
   - name: zededa-tools
     image: ZEDEDA_TAG
trust:
  org:
    - linuxkit

